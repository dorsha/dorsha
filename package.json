{
	"name": "dorsha",
	"version": "0.0.1",
	"private": true,
	"author": "dorsha",
	"scripts": {
		"clear": "rm -rf build/*",
		"dev": "webpack serve --env mode=development",
		"format": "prettier --ignore-path .gitignore --write .",
		"format-check": "prettier --ignore-path .gitignore --check .",
		"format-lint": "pretty-quick --staged --ignore-path .gitignore && lint-staged",
		"lint": "eslint .",
		"lint-fix": "eslint --fix .",
		"prepare": "husky install",
		"prod": "NODE_ENV=production webpack -- --env mode=production",
		"prod:server": "NODE_ENV=production webpack serve --env mode=production --no-client-overlay --history-api-fallback",
		"webpack": "webpack",
		"webpack-dev-server": "webpack-dev-server"
	},
	"lint-staged": {
		"src/**/*.{js,ts,jsx,tsx}": [
			"npm run lint"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"bootstrap-css": "^4.0.0-alpha.5",
		"lodash": "4.17.21",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typed.js": "^2.0.12"
	},
